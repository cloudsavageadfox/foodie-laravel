            function showMenu()
            {
                $("#hidden-header").css("height", "100px");
            }

            function hideMenu()
            {
                $("#hidden-header").css("height", "0");
            }

            var headerShown = true;

            $(window).on("scroll", function ()
            {
                var header = $(".main-page-header");
                if ($(window).scrollTop() > (header.offset().top + header.height()))
                {
                    if (headerShown)
                    {
                        showMenu();
                        headerShown = false;
                    }
                }
                else
                {
                    if (!headerShown)
                    {
                        hideMenu();
                        headerShown = true;
                    }
                }
            });

