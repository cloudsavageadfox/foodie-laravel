        @if (youtubeExists)
        {
            <script type="text/javascript">

            // 1. This code loads the IFrame Player API code asynchronously.
            var tag = document.createElement('script');

            tag.src = "https://www.youtube.com/iframe_api";
            var firstScriptTag = document.getElementsByTagName('script')[0];
            firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

            // 2. This function creates an <iframe> (and YouTube player)
            //    after the API code downloads.
            var player;
            function onYouTubeIframeAPIReady() {
                @if (media != null)
                {
                    foreach (Media item in media)
                    {
                        if (item.MediaType == Core.Enums.MediaType.Video)
                        {
                            if (!string.IsNullOrEmpty(item.Link))
                            {
                                string videoId = item.Link.Substring(item.Link.IndexOf("v=") + 2);
                                if (videoId.Contains("&"))
                                {
                                    videoId = videoId.Substring(0, videoId.IndexOf("&"));
                                }
                                    <text>
                                        player@(item.Id) = new YT.Player('player@(item.Id)', {
                                        height: '100%',
                                        width: '100%',
                                        playerVars: {
                                            autoplay: true,
                                            loop: true,
                                            controls: 0,
                                            showinfo: 0,
                                            autohide: 1,
                                            modestbranding: 1,
                                            vq: 'default',
                                            playlist: '@videoId'
                                        },
                                        videoId: '@videoId',
                                        events: {
                                            'onReady': onPlayerReady@(item.Id),
                                            'onStateChange': onPlayerStateChange
                                        }
                                    });

                                </text>
                            }
                        }
                    }
                }
            }

            @if (media != null)
            {
                foreach (Media item in media)
                {
                    if (item.MediaType == Core.Enums.MediaType.Video)
                    {
                        if (!string.IsNullOrEmpty(item.Link))
                        {
                            <text>
                                // 3. The API will call this function when the video player is ready.
                                function onPlayerReady@(item.Id)(event) {
                                    event.target.playVideo();
                                    player@(item.Id).mute();
                                }
                            </text>
                        }
                    }
                }
            }
            function onPlayerStateChange(event) {

            }

            </script>
        }
